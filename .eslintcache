      setIsLoading(false)\r\n                console.log(error)\r\n            }\r\n        };\r\n    },[])\r\n\r\n    function showingComment(comment) {\r\n\r\n        if (comment != null) {\r\n            return (\r\n                <div className={styles['showCommentDiv']}>\r\n\r\n                    Comment:\r\n                    <textarea className={styles['viewCommentViewSubmission']} value={comment.message}> </textarea>\r\n\r\n                </div>\r\n\r\n            )\r\n\r\n        }\r\n    }\r\n\r\n    function showingCommentOptions(comment) {\r\n\r\n        if (comment != null) {\r\n            return (\r\n\r\n\r\n                <CommentOptions song={song} comment={comment} userId={userData.data.userId}/>\r\n\r\n            )\r\n\r\n\r\n        } else {\r\n            return (\r\n\r\n                <CommentOptions song={song} comment={comment} userId={userData.data.userId}/>\r\n\r\n            )\r\n        }\r\n\r\n    }\r\n\r\n    return (\r\n        !isLoading && demoData && song ?\r\n            (\r\n                <>\r\n                    <NavigationBar/>\r\n                    <div className={styles['containerViewSubmission']}>\r\n                        <h1>{demoData.data.artist} - {demoData.data.songTitle}</h1>\r\n                        <table className={styles['tableViewSubmission']}>\r\n                            <tr>\r\n                                <th>Firstname</th>\r\n                                <th>Lastname</th>\r\n                                <th>Email</th>\r\n                                <th>Country</th>\r\n                                <th>Facebook</th>\r\n                                <th>Instagram</th>\r\n\r\n                            </tr>\r\n                            <tr>\r\n                                <td>{userData.data.firstName}</td>\r\n                                <td>{userData.data.lastName}</td>\r\n                                <td>{userData.data.email}</td>\r\n                                <td>{userData.data.country}</td>\r\n                                <td>{userData.data.facebook}</td>\r\n                                <td>{userData.data.instagram}</td>\r\n                            </tr>\r\n                        </table>\r\n                        <div className={styles['spaceDivViewComment']}></div>\r\n                        {showingComment(comment)}\r\n                        {showingCommentOptions(comment)}\r\n\r\n                        <SongLoader\r\n                            song={demoData.data}\r\n                        />\r\n\r\n                    </div>\r\n                </>\r\n            ) : (\r\n                <>\r\n                    <NavigationBar/>\r\n\r\n                    <h1> teasdsadsad</h1>\r\n                </>\r\n            )\r\n\r\n\r\n    );\r\n\r\n}\r\n\r\nexport default ViewSubmission;","C:\\Users\\remco\\WebstormProjects\\EindOefening1.js\\src\\pages\\SignUp\\SignUp.js",[],"C:\\Users\\remco\\WebstormProjects\\EindOefening1.js\\src\\pages\\Comment\\WriteComment.js",["200"],"import React, { useEffect, useState } from 'react'\r\nimport { useHistory } from 'react-router-dom'\r\nimport axios from \"axios\";\r\nimport NavigationBar from \"../../components/NavigationBar/NavigationBar\";\r\nimport SongLoader from \"../../components/songLoader/SongLoader\";\r\n\r\nimport styles from './Comment.module.css'\r\n\r\n\r\nfunction Writecomment(){\r\n\r\n    const urlParams = new URLSearchParams(window.location.search);\r\n    var urlParameters = [];\r\n    const [comment, setComment] = useState({})\r\n    const [song, setSong] = useState(null)\r\n    const history = useHistory()\r\n    const [isLoading, setIsLoading] = useState(false)\r\n    const [textComment, setTextComment] = useState();\r\n    const [userData, setUserData] = useState();\r\n\r\n    for (const param of urlParams) {\r\n        urlParameters.push(param);\r\n    }\r\n    let songId = urlParameters[0][1];\r\n    let userId = urlParameters[1][1];\r\n\r\n\r\n\r\n\r\n    useEffect(() => {\r\n        fetchData()\r\n        async function fetchData() {\r\n            setIsLoading(true)\r\n            try {\r\n\r\n                const demoDataResponse = await axios.get(`http://localhost:8080/api/demos/${songId}`, {\r\n                    headers: {\r\n                        'Authorization': localStorage.getItem('token')\r\n                    }\r\n                })\r\n\r\n\r\n                    const userDataResponse = await axios.get(`http://localhost:8080/api/user/${userId}`, {\r\n                        headers: {\r\n                            'Authorization': localStorage.getItem('token')\r\n                        }\r\n                    })\r\n\r\n\r\n                setIsLoading(false)\r\n                setSong(demoDataResponse);\r\n\r\n                setUserData(userDataResponse.data);\r\n                const newComment = {}\r\n                newComment.demoId = songId\r\n                newComment.message = ''\r\n\r\n        } catch (error) {\r\n                // TODO User error message\r\n                setIsLoading(false)\r\n                console.log(error)\r\n            }\r\n\r\n        }\r\n\r\n    }, [songId])\r\n\r\n    function handleChange(event) {\r\n        const updatedComment = { ...comment }\r\n        updatedComment.message = event.target.value\r\n        updatedComment.demoId = songId\r\n        setComment(updatedComment)\r\n        setTextComment(event.target.value);\r\n    }\r\n    async function handleSave() {\r\n\r\n        setIsLoading(true)\r\n        try {\r\n\r\n            await axios.post(`http://localhost:8080/api/comments/` , comment , {\r\n                headers: {\r\n                    'Authorization': localStorage.getItem('token')\r\n                }\r\n            })\r\n            setIsLoading(false)\r\n            history.push(`/viewComment?songId=${songId}`)\r\n\r\n        } catch (error) {\r\n            // TODO User error message\r\n            setIsLoading(false)\r\n            console.log(error)\r\n        }\r\n    }\r\n\r\n    function jsFunction(value){\r\n        let localComment = '';\r\n        if(value === \"1\"){\r\n            localComment = `Dear ${userData.fullName}, \r\n                 \r\nThanks for sending you track: ${song.data.artist} - ${song.data.songTitle}!\r\n\r\nWe listened to it, and its a dope track.\r\n \r\nThats why we wanna invite you to meet us at our location.\r\n\r\nWe will send u an invitation within a week.\r\n\r\nKind regards,\r\n\r\nTeam Don Diablo.\r\n`;\r\n        }\r\n        else if(value === \"2\"){\r\n            localComment = `Dear ${userData.fullName}, \r\n                 \r\nThanks for sending you track: ${song.data.artist} - ${song.data.songTitle}!\r\n\r\nIts a decent track, but we don't see us using it or working with it.\r\n\r\nWe thank you for your time and wish you good luck in the future.\r\n\r\nKind regards,\r\n\r\nTeam Don Diablo.\r\n`;\r\n        }\r\n        else{\r\n            localComment = `Dear ${userData.fullName}, \r\n                 \r\nThanks for sending you track: ${song.data.artist} - ${song.data.songTitle}!\r\n\r\nWe didn't have the time to listen to it yet.\r\n\r\nThats why we need more time.\r\n\r\nWe will send an response wihtin a week. \r\n\r\nKind regards,\r\n\r\nTeam Don Diablo.\r\n`;\r\n\r\n        }\r\n\r\n\r\n        const updatedComment = {\r\n            message : localComment,\r\n            demoId: songId\r\n        }\r\n        setComment(updatedComment)\r\n        setTextComment( localComment);\r\n\r\n    }\r\n\r\n\r\n\r\n\r\n    return(\r\n        !isLoading && song ?\r\n            (\r\n                <div>\r\n                    <NavigationBar/>\r\n                    <div  className={styles['writeCommentContainer']}>\r\n                        <select defaultValue={'default'} name=\"type\" onChange={e => jsFunction(e.target.value) }>\r\n                            <option value=\"default\" disabled>Choose an option</option>\r\n      